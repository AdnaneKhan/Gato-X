{
    "name": "WORKFLOW_PARSING",
    "entries": {
        "PERMISSION_CHECK_ACTIONS": [
            "check-actor-permission"
        ],
        "SAFE_IF_CHECKS": [
            "github.event.pull_request.merged == true",
            "== labeled",
            "== 'labeled'",
            "github.event.pull_request.head.repo.fork != true"
        ],
        "PR_ISH_VALUES": [
            "head",
            "pr",
            "pull",
            "merge",
            "ref",
            "sha",
            "issue",
            "number"
        ],
        "GITHUB_HOSTED_LABELS": [
            "ubuntu-latest",
            "macos-latest",
            "macOS-latest",
            "windows-latest",
            "ubuntu-18.04",
            "ubuntu-20.04",
            "ubuntu-22.04",
            "ubuntu-24.04",
            "windows-2022",
            "windows-2019",
            "windows-2016", 
            "macOS-13",
            "macOS-12",
            "macOS-11",
            "macos-11",
            "macos-12",
            "macos-13",
            "macos-13-xl",
            "macos-13-xlarge",
            "macos-latest-xlarge",
            "macos-12",
            "macos-14",
            "macos-14-large",
            "macos-14-xlarge"
        ],
        "PR_REVIEW_UNSAFE_CONTEXTS": [
            "github.event.review.body",
            "github.event.comment.body"
        ],
